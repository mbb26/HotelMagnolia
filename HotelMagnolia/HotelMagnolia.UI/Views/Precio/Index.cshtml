@model IEnumerable<HotelMagnolia.UI.Models.PRECIO>

@{
    ViewBag.Title = "Index";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TIPO_PRECIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRECIO1)
        </th>
        <th colspan="3">Acciones</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TIPO_PRECIO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PRECIO1)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.ID_PRECIO })
            </td>
            <td>
                @Html.ActionLink("Detalles", "Details", new { id = item.ID_PRECIO })
            </td>
            <td>
                @Html.ActionLink("Eliminar", "Delete", new { id = item.ID_PRECIO })
            </td>
        </tr>
    }

</table>

<p>
    @Html.ActionLink("Crear Nuevo", "Create", null, new { @class = "btn btn-primary" })
</p>