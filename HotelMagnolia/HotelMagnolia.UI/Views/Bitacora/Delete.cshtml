@model HotelMagnolia.UI.Models.BITACORA

@{
    ViewBag.Title = "Eliminar Bitacora";
}

<h2>@ViewBag.Title</h2>

¿Confirma que desea eliminar esto?</h3>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div>
        <h4>BITACORA</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Fecha
            </dt>

            <dd>
                @Html.DisplayFor(model => model.FECHA)
            </dd>

            <dt>
                Codigo
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CODIGO)
            </dd>

            <dt>
                Descipcion
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Descripcion)
            </dd>

            <dt>
                Detalle
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Registro_en_detalle)
            </dd>

        <dt>
            Tipo Bitacora
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TIPO_BITACORA.NOMBRE)
        </dd>

        <dt>
            Usuario
        </dt>

        <dd>
            @Html.DisplayFor(model => model.USUARIO.NOMBRE)
        </dd>

        </dl>

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Regresar", "Index", null,new { @class = "btn btn-primary" })
        </div>
    </div>

}