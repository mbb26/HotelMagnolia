@model IEnumerable<HotelMagnolia.UI.Models.HABITACION>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            Numero
        </th>
        <th>
            Nombre
        </th>
        <th>
            Descripcion
        </th>
        <th>
            Foto
        </th>
        <th>
            Precio
        </th>
        <th>
            Tipo
        </th>
        <th colspan="3">Acciones</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID_HABITACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NUMERO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESCRIPCION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FOTO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRECIO.TIPO_PRECIO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TIPO_HABITACION1.NOMBRE)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_HABITACION }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_HABITACION }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_HABITACION })
        </td>
    </tr>
}

</table>
