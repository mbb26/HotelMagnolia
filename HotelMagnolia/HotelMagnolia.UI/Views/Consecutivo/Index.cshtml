@model IEnumerable<HotelMagnolia.UI.Models.CONSECUTIVO>

@{
    ViewBag.Title = "Lista de Consecutivos";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            Nombre
        </th>
        <th>
            Valor
        </th>
        <th>
            Tiene Prefijo
        </th>
        <th>
            Prefijo
        </th>
        <th>
            Posee Rango
        </th>
        <th>
            Rango Inicial
        </th>
        <th>
            Rango Final
        </th>
        <th colspan="3">Acciones</th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.NOMBRE)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.VALOR)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TIENE_PREFIJO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PREFIJO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.POSEE_RANGO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.RANGO_INICIAL)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.RANGO_FINAL)
    </td>
    <td>
        @Html.ActionLink("Editar", "Edit", new { id = item.ID_CONSECUTIVOS })
    </td>
    <td>
        @Html.ActionLink("Detalles", "Details", new { id = item.ID_CONSECUTIVOS })
    </td>
    <td>
        @Html.ActionLink("Eliminar", "Delete", new { id = item.ID_CONSECUTIVOS })
    </td>
</tr>
}

</table>

<p>
    @Html.ActionLink("Crear Nuevo", "Create", null, new { @class = "btn btn-primary" })
</p>
